global:
  resolve_timeout: 1m

route:
  group_by: ['alertname']
  group_wait: 10s
  group_interval: 10s
  repeat_interval: 10m
  receiver: 'web.hook'
  
receivers:
  - name: 'web.hook'
    webhook_configs:
    - url: 'http://<%= @executor_listen %>:<%= @executor_port %>/'
      send_resolved: true

